<Window x:Class="CookieRunKingdom.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CookieRunKingdom"
        mc:Ignorable="d"
        Title="MainWindow" Height="756" Width="1062" Loaded="Window_Loaded">
    <Grid>
        <ComboBox x:Name="cbProcessList" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="298" SelectionChanged="cbProcessList_SelectionChanged"/>
        <Image x:Name="imgCapture" HorizontalAlignment="Left" Margin="10,37,0,0" VerticalAlignment="Top" Height="598" Width="800"/>
        <Button x:Name="btnCapture" Content="Capture" HorizontalAlignment="Left" Margin="313,10,0,0" VerticalAlignment="Top" Height="22" Click="btnCapture_Click"/>

    </Grid>
</Window>
